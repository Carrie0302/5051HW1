@model HW1.Models.HomeViewModel
@{
    ViewBag.Title = "Home Page";

}

<div class="jumbotron">
    <h1>Carrie Schaden</h1>
    <img class="rounded img-thumbnail img-fluid w-50 mx-auto d-block" src="~/Content/Img/windyarizona.jpg" />
</div>

<div class="row">
    <div class="col text-center">
        <h2>Number of Phones</h2>
        <h3>@Model.PhoneCount</h3>
    </div>
    <div class="col text-center">
        <h2>Number of Users</h2>
        <h3>@Model.UserCount</h3>
    </div>

    <div class="col text-center">
        <h2>Last Access Date</h2>
        <h3>@Model.LastAcessDate.ToShortDateString()</h3>
    </div>

</div>